{
  "name": "mira-devtools",
  "main": "electron/main.js",
  "homepage": "./",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Hoang Nguyen",
    "email": "hoangnh17193@gmail.com"
  },
  "scripts": {
    "start": "export PORT=7777 BROWSER=none && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:start": "electron .",
    "electron:dev": "concurrently \"npm start\" \"wait-on http://localhost:7777 && electron .\"",
    "electron:build": "electron-builder"
  },
  "dependencies": {
    "@ant-design/icons": "^4.5.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "antd": "^4.13.1",
    "custom-electron-titlebar": "^3.2.6",
    "dayjs": "^1.10.4",
    "electron-is-dev": "^2.0.0",
    "install": "^0.13.0",
    "lodash": "^4.17.21",
    "node-sass": "^5.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-json-view": "^1.21.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-syntax-highlighter": "^15.4.3",
    "redux": "^4.0.5",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "concurrently": "^6.0.0",
    "electron": "^12.0.0",
    "electron-builder": "^22.10.5",
    "wait-on": "^5.2.1"
  },
  "build": {
    "appId": "com.tino.-  app",
    "productName": "Mira Devtools",
    "copyright": "Copyright Â© 2021 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "electron/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "extends": null
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
